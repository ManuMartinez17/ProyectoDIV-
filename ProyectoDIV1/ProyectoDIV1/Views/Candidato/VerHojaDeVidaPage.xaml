<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:syncfusion="clr-namespace:Syncfusion.SfPdfViewer.XForms;assembly=Syncfusion.SfPdfViewer.XForms"
             x:Class="ProyectoDIV1.Views.Candidato.VerHojaDeVidaPage">
    <ContentPage.Content>
        <Grid>
            <syncfusion:SfPdfViewer x:Name="pdfViewerControl" IsPasswordViewEnabled="False"  PageViewMode="PageByPage" InputFileStream="{Binding PdfDocumentStream}"/>
        </Grid>
    </ContentPage.Content>
</ContentPage>